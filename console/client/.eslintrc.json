{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "modules": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": 1,
    "no-extra-semi": 1,
    "semi": [2, "never"],
    "no-fallthrough": 1,
    "no-empty": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-redeclare": 1,
    "no-this-before-super": 1,
    "no-undef": 1,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "constructor-super": 1,
    "curly": 1,
    "eqeqeq": 1,
    "func-names": 1,
    "valid-typeof": 1
  }
}